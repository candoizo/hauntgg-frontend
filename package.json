{
  "name": "haunt.gg",
  "scripts": {
    "gulp": "gulp --cwd . -f assets/gulpfile.js",
    "docker": "[[ $NODE_ENV == release ]] && echo '\n  [!] NODE_ENV=release'; cd docker && docker-compose",
    "update-browserslist": "npx browserslist@latest --update-db"
  },
  "husky": {
    "hooks": {
      "pre-commit": "if [[ $(git rev-parse --abbrev-ref HEAD) = preview ]] || [[ $(git rev-parse --abbrev-ref HEAD) = release ]]; then echo \"‚ùé Only merges allowed directly to: $(git rev-parse --abbrev-ref HEAD)\" && exit 1; else yarn run eslint . && rm -f CHANGELOG.md && yarn run gitmoji-changelog && git add CHANGELOG.md; fi",
      "commit-msg": "[ -f .git/MERGE_MSG ] && sed -i 's/^Merge/:twisted_rightwards_arrows: merge:/g' .git/MERGE_MSG; commitlint -E HUSKY_GIT_PARAMS || exit 1; [[ $(git rev-parse --abbrev-ref HEAD) = release ]] && yarn run semantic-release || exit 0"
    }
  },
  "author": "candoizo <candoizo@protonmail.com>",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.com/hauntgg/hauntgg-frontend.git"
  },
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@gitmoji-changelog/cli": "^1.0.1",
    "@metamask/detect-provider": "^1.2.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.0",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "browserslist": "^4.16.3",
    "commitlint-config-gitmoji": "2.2.3",
    "css-declaration-sorter": "^6.0.2",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "debug": "^4.3.1",
    "eslint": "^7.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-md": "^1.0.19",
    "eslint-plugin-node": "^11.1.0",
